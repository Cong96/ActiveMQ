<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="com.wangcc.ssm.dao.PlayerDao">


<insert id="insertPlayer" parameterType="com.wangcc.ssm.entity.Player">
insert into player(id,name,age) values(#{id},#{name},#{age})
</insert>
<select id="selectById" parameterType="Integer" resultType="com.wangcc.ssm.entity.Player">
select * from player where id=#{id}
</select>
</mapper>